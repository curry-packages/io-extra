{
    "name": "io-extra",
    "version": "3.0.0",
    "author": "Michael Hanus <mh@informatik.uni-kiel.de>",
    "synopsis": "Library with some useful extensions to the IO monad.",
    "category": [ "IO" ],
    "license": "BSD-3-Clause",
    "licenseFile": "LICENSE",
    "dependencies": {
          "base":     ">= 3.0.0, < 4.0.0",
